    <add key="readRemainEle" value="1UxoNXlwHQOg4GxlS20y8vDEcVu" />
    <!--电接口配置end-->
    
    <!--水接口配置start-->
    <!-- 获取水接口token 账号-->
    <add key="W_UserName" value="123" />
